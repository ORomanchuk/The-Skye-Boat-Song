#include <NanitLib.h>

byte buzz = P4_2;
byte WhiteLed = P4_4;
byte BlueLed = P4_3;
byte GreenLed = P5_4;
byte YellowLed = P5_3;
byte RedLed = P5_2;

void setup() {
  Nanit_Base_Start();
  GET_NANIT.getBataryVoltage();
  pinMode(buzz, OUTPUT);
  pinMode(WhiteLed, OUTPUT);
  pinMode(BlueLed, OUTPUT);
  pinMode(GreenLed, OUTPUT);
  pinMode(YellowLed, OUTPUT);
  pinMode(RedLed, OUTPUT);
}

void loop() {
  tone(buzz, 294);
  digitalWrite(WhiteLed, 1);
  digitalWrite(BlueLed, 0);
  digitalWrite(GreenLed, 0);
  digitalWrite(YellowLed, 0);
  digitalWrite(RedLed, 0);
  delay(562);
  tone(buzz, 330);
    digitalWrite(WhiteLed, 1);
    digitalWrite(BlueLed, 1);
    digitalWrite(GreenLed, 0);
    digitalWrite(YellowLed, 0);
    digitalWrite(RedLed, 0);
  delay(188);
  tone(buzz, 294);
    digitalWrite(WhiteLed, 1);
    digitalWrite(BlueLed, 0);
    digitalWrite(GreenLed, 0);
    digitalWrite(YellowLed, 0);
    digitalWrite(RedLed, 0);
