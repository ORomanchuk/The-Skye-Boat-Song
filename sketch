  tone(buzz, 392);
    digitalWrite(WhiteLed, 1);
    digitalWrite(BlueLed, 1);
    digitalWrite(GreenLed, 1);
    digitalWrite(YellowLed, 0);
    digitalWrite(RedLed, 0);
